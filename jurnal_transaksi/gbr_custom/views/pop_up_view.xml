<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
<record model="ir.ui.view" id="view_stock_quant_tree_popup">
    <field name="name">stock.quant.tree.popup</field>
    <field name="model">stock.quant</field>
    <field eval="12" name="priority"/>
    <field name="arch" type="xml">
        <tree string="Sisa Stock" js_class="inventory_report_list"
              create="0" delete="0" decoration-danger="quantity &lt; 0">
            <field name="product_id" required="0" invisible="1"/>
            <field name="location_id" required="0" string="Gudang"/>    
            <field name="quantity" string="On Hand Quantity" invisible="1"/>
            <field name="inventory_quantity" string="Sisa AC" readonly="1"/>
            <field name="reserved_quantity" invisible="1"/>
            <field name="product_uom_id" groups="uom.group_uom"/> 
        </tree>
    </field>
</record>
<record id="view_move_line_tree_popup" model="ir.ui.view">
    <field name="name">account.move.line.tree.popup</field>
    <field name="model">account.move.line</field>
    <field eval="1" name="priority"/>
    <field name="arch" type="xml">
        <tree string="Journal Items" create="false" expand="context.get('expand', False)" multi_edit="1">
            <field name="date" optional="show" readonly="1"/>
            <field name="company_id" groups="base.group_multi_company" options="{'no_create': True}" optional="show"/>
            <field name="account_id" optional="show" options="{'no_open': True, 'no_create': True}"
                   domain="[('company_id', '=', company_id)]"
                   groups="account.group_account_user"/>    
            <field name="name"/>
            <field name="reconciled" invisible="1"/>
            <field name="full_reconcile_id" string="Matching" optional="show"/>
            <field name="reconcile_model_id" invisible="1"/>
            <field name="date_maturity" optional="hide"/>
            <field name="debit" sum="Total Debit" readonly="1"/>
            <field name="credit" sum="Total Credit" readonly="1"/>
            <field name="amount_currency" readonly="1" groups="base.group_multi_currency"/>
            <field name="currency_id" readonly="1" invisible="1" />
            <field name="company_currency_id" invisible="1"/>
            <field name="parent_state" invisible="1"/>          
        </tree>
    </field>
</record>

</data>
</odoo>