<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<data>

	<record id="view_pelanggan_gbr_tree" model="ir.ui.view">
		<field name="name">view.pelanggan.gbr.tree</field>
		<field name="model">res.partner</field>
		<field name="priority">99</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree js_class="pelanggan_tree">
				<field name="is_customer" invisible="1"/>
				<field name="id" optional="show"/>
				<field name="date" string="Tanggal" optional="show"/>
				<field name="non_active" optional="show"/>
				<field name="npwp" optional="show"/>
				<field name="name" optional="show"/>
				<field name="address" optional="show"/>
				<field name="state_id" string="Daerah" optional="show"/>
				<field name="country_id" optional="show"/>
				<field name="phone" optional="show"/>
				<field name="phone2" string="Telp(2)" optional="show"/>
				<field name="phone3" string="Telp(3)" optional="show"/>
				<field name="fax" optional="show"/>
				<field name="contact_person1" string="Hubungi" optional="show"/>
				<field name="contact_number1" string="Hub-Telp" optional="show"/>
				<field name="contact_person2" string="Hubungi *2" optional="show"/>
				<field name="contact_number2" string="Hub-Telp *2" optional="show"/>
				<field name="contact_person3" string="Hubungi *3" optional="show"/>
				<field name="contact_number3" string="Hub-Telp *3" optional="show"/>
				<field name="email" optional="show"/>
				<field name="website" string="Website" optional="show"/>
				<field name="comment" optional="show"/>
				<field name="global_dis" optional="show"/>
				<field name="item_disc" optional="show"/>
				<field name="kurs" optional="show"/>
				<field name="property_supplier_payment_term_id" optional="show"/>
				<field name="nominal_maxCrd" optional="show"/>
				<field name="nominal_maxCrd2" optional="show"/>
				<field name="protect1" optional="show"/>
				<field name="max_payment_term_credit" optional="show"/>
				<field name="max_payment_term_credit2" optional="show"/>
				<field name="protect2" optional="show"/>
				<field name="max_payment_term" optional="show"/>
				<field name="pricelist_id" optional="show"/>
				<field name="type_sale_id" optional="show"/>
				<field name="department_id" optional="show"/>
				<field name="religion" optional="hide"/>
				<field name="default_mu_jual_id" string="Mu-Jual" optional="hide"/>
				<field name="write_uid" string="Entry-by" optional="hide"/>
				<field name="create_date" string="Create-Date" optional="hide"/>
				<field name="write_date" string="Last-Edit Date" optional="hide"/>
			</tree>
		</field>
	</record>

	<record id="view_pelanggan_gbr_form" model="ir.ui.view">
		<field name="name">view.pelanggan.gbr.form</field>
		<field name="model">res.partner</field>
		<field name="priority">99</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
