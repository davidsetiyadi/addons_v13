            				</div>
            				<div>
                				<field name="contact_person3" class="oe_inline" placeholder="Name"/>&amp;nbsp; &amp;nbsp;
                				<field name="contact_number3" class="oe_inline" placeholder="Phone" />
            				</div>
            			</div>	

            			<field name="ktp" string="No KTP" attrs="{'invisible': [('is_company','=',True)]}"/>
            			<label for="born" attrs="{'invisible': [('is_company','=',True)]}"/>
            			<div attrs="{'invisible': [('is_company','=',True)]}">
            				<field name="born" class="oe_inline"/>&amp;nbsp; &amp;nbsp;
            				<field name="birth" class="oe_inline"/>
            			</div>
            			<field name="status" attrs="{'invisible': [('is_company','=',True)]}"/>
            			<field name="pend_akh" attrs="{'invisible': [('is_company','=',True)]}"/>

                		<label for="property_supplier_payment_term_id" string="Credit"/>
