            			</div>
            			
            			<field name="sales_employee_id" string="Sales" domain="[('is_sales','=',True)]"  options='{"no_quick_create": True,"no_create": True,"no_open": True}'/>
            			<field name="collector_employee_id" string="Collector" domain="[('is_collector','=',True)]" options='{"no_quick_create": True,"no_create": True,"no_open": True}'/>
            			<label for="type_sale_id"/>
