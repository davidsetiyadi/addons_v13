            				<field name="contact_person1" class="oe_inline" placeholder="Name" />&amp;nbsp; &amp;nbsp;	            
            				<field name="contact_number1" class="oe_inline" placeholder="Phone"/>
            				<div>
                				<field name="contact_person2" class="oe_inline" placeholder="Name"/>&amp;nbsp; &amp;nbsp;
