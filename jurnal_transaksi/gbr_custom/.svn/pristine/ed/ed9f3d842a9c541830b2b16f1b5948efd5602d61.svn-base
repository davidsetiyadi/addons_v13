                				<field name="country_id" placeholder="Negara" required="1" class="oe_inline" style="padding-right: 10px;" context="{'default_country_id': 100}"/>&amp;nbsp; &amp;nbsp;
                				<field name="religion" placeholder="---" class="oe_inline" attrs="{'invisible': [('is_company','=',True)]}"/>
                			</div>
                			<label for="state_id" string="Daerah"/>
                			<div>
	                			<field name="state_id" string="Daerah" domain="[('country_id','=',country_id)]" style="padding-right: 10px;"/>
	                			<field name="kota_id" string="Kota" domain="[('state_id','=',state_id)]" style="padding-right: 10px;"/>
	                			
	                		</div>
	                		<label for="kecamatan_id" string="Kecamatan"/>
                			<div>
                				<field name="kecamatan_id" string="kecamatan_id" invisible="0" domain="[('kota_id','=',kota_id)]" style="padding-right: 10px;"/>
                				<field name="kelurahan_id" string="Kelurahan" invisible="0" domain="[('kecamatan_id','=',kecamatan_id)]" style="padding-right: 10px;"/>
                			</div>
                			
                			<field name="npwp"/>
                			<label for="phone" string="Telephone"/>
