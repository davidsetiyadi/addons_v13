	         				<label for="is_black_list" class="oe_inline" string="Penjualan : Daftar Pelanggan di black list"/>
	         				<field name="is_limit" class="oe_inline"/>
	         				<label for="is_limit" class="oe_inline"/>
         				</div>				         				
         		</group>

