                			<field name="type_sale_id"/>
                			<field name="department_id"/>
            			</div>
                		</group>
            		</group>		                		
            	</page> 
            	<page name="inisialisasi" string="Inisialisasi" invisible="0">		                		
            		<group name="inisialisasi" col="12">
            			<notebook name="group_user_setting">
            				<page string="Inisialisasi">
                				<group colspan="5">
		                			<label for="pajak_type"/>
