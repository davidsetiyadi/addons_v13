            				<field name="property_supplier_payment_term_id" placeholder="0 hari" class="oe_inline"/>&amp;nbsp; &amp;nbsp;
            				<field name="term_cred" placeholder="+ 0 EOM" class="oe_inline"/>
            			</div>
            			<label for="global_dis"/>
