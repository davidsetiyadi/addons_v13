        					   'form_view_ref': 'gbr_custom.view_pelanggan_gbr_form',
        					   'default_customer_rank': 1,'default_is_customer': 1, 'default_is_company': 1,
        					   'search_default_customer': 1,'res_partner_search_mode': 'customer' ,'default_country_id': 100,
