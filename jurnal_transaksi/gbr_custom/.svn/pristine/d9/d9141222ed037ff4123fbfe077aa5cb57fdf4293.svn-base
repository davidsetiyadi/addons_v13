        <field name="model">gbr.invoice.statement</field>
        <field name="priority">99</field>
        <field name="arch" type="xml">
            <form string="Rekap">               
                <sheet>                    
                    <group>
                        <group id="header_left_group">
                           <field name="partner_id" required="1" options="{'no_create': True,'no_quick_create': True, 'no_open': True}"/>
                           <field name="employee_id" required="0" options="{'no_create': True,'no_quick_create': True, 'no_open': True}"/>
                           <field name="active" optional="hide" invisible="1"/>
                        </group>
                        <group id="header_right_group">
                            <field name="divisi_id" required="1" options="{'no_create': True,'no_quick_create': True, 'no_open': True}"/>
                            <label for ="no" string="NO"/>
                            <div>
                                <field name="no" string="NO" readonly="1" style="padding-right:10px; "/>
                                <field name="no_append"  readonly="0" style="background:wheat;"/>
                            </div>
                            <field name="date_start" string="Tanggal" required="1"/>
                            <field name="currency_id" options="{'no_create': True,'no_quick_create': True}"/>
                            <!-- <field name="date_end" string="Tanggal"/> -->
                           
                        </group>
                    </group>
                    <notebook>                        
                        <page id="other_tab" string="List" name="other_info">
                            <field name="line_ids" >
                                <tree editable="bottom" create="false">
                                    <control>
                                        <create name="add_line_control" string="Tambah" class="oe_highlight"/>
                                    </control>
                                    <field name="id" string="ID" optional="hide"/>

                                    <field name="type_code" string=" " optional="show" style="width: 49px;"/>
                                    <field name="no" string="NO" optional="show"/>
                                    <field name="invoice_id" optional="hide" invisible="1"/>
                                    <field name="ref" optional="hide" readonly="1"/>
                                    <field name="invoice_date" optional="show" readonly="1"/>
                                    <field name="partner_id" optional="hide" readonly="0"/>
                                    <field name="currency_id" optional="show" readonly="1"/>
                                    <field name="harga_jual" optional="show" readonly="1"/> 
                                    <field name="discount" optional="show" readonly="1"/>   
                                    <field name="nilai_jual" widget="monetary" optional="show" readonly="1"/>   
                                    <field name="invoice_payment_term_id" optional="show" readonly="1"/>
                                    <field name="invoice_date_due" optional="show" readonly="1"/>
                                    <field name="sale_2_id" optional="show" readonly="1"/>
                                    <field name="purchase_char_id" optional="show" readonly="1"/>
                                    <field name="divisi_id" optional="show" readonly="1"/>                                             
                                    <field name="write_uid" string="Entry-By" optional="hide"/>
                                    <field name="create_date" string="Entry-date" optional="hide"/>
                                    <field name="write_uid" string="Last-Edit By" optional="hide"/>
                                    <field name="write_date" string="Last-Edit Date" optional="hide"/>
                                </tree>
                                <form>
                                    <field name="invoice_id" domain="[('partner_id','=',parent.partner_id)]"/>
                                </form>
                            </field>
                            <button type="action" name="%(gbr_custom.action_wizard_statement_filter_view)d" string="Tambah" class="btn btn-info"/>
                                <br/><br/><br/>                 
                            <field name="catatan" placeholder="Catatan"/>
                            <field name="catatan_2" placeholder="Catatan"/>                       
                        </page>
                        
                    </notebook>
                </sheet>                
                
                <!-- Chatter -->
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>
