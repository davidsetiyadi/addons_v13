<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for rek.peng.brg.penj -->
        <record id="seq_rek_penj" model="ir.sequence">
            <field name="name">Rek Peng Brg Penj</field>
            <field name="code">rek.peng.brg.penj</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record forcecreate="True" id="ir_cron_reset_sequence_action" model="ir.cron">
            <field name="name">GBR Custom: run reset sequence</field>
            <field name="model_id" ref="model_gbr_divisi"/>
            <field name="state">code</field>
            <field name="code">
                model.run_reset_sequence()
            </field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>

    </data>
</odoo>
